<% if (!id) { %>
<section>
	<div class="progress grid">
		<div class="cell active actived"><p></p><span></span><em>网点查询</em></div>
		<div class="cell active actived"><p></p><span></span><em>订单填写</em></div>
		<div class="cell active"><p></p><span></span><em>订单完成</em></div>
	</div>
</section>
<% } %>
<div class="modle">
	<h4>兑换内容</h4>
	<p>兑换类型: <%= type  %></p>
	<p>外币类型: <%= currency %></p>
	<p>兑换金额: <%= sum %></p>
	<p>取钞日期: <%= takeDate %></p>
</div>
<div class="modle">
	<h4>兑换人</h4>
	<p>兑换人姓名: <%= changer %></p>
	<p>证件类型: <%= idTypeText %></p>
	<p>证件号码：<%= idNo %></p>
	<p>联系手机：<%= mobileNo %></p>
</div>
<div class="modle">
	<h4>网点信息</h4>
	<p>城市:<%= city %></p>
	<p>地址: <%= address %></p>
	<p>营业时间: <%= opentime %></p>
</div>
<% if (!id) { %>
  <a href="javascript:;" class="button btn-order" id="btn-order">确认提交</a>
<% } else { %>
	<div class="btn-view">
		<% if(orderStatus == 1) {%>
			<div><span class="cancel">取消</span></div>
		<% } %>
		<div><span class="back">返回</span></div>
	</div>
<% } %>