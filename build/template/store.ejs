<% if (!list.length) {%>
	<li class="none">无满足条件的兑换网点，请返回重新查询</li>
<% } %>
<% for(var i=0,len=list.length;i<len;i++){ %>
	<li data-index="<%= i%>">
		<% if(i==0){ %>
			<em class="tag">最优</em>
		<% } %>
		<h4><%= list[i].name%></h4>
		<p>兑换汇率: <%=list[i].rateText %></p>
		<p>单笔手续费: <%=list[i].change %>元</p>
		<p>单笔订单金额上下限（人民币）: <%=list[i].min %>元 -
		<% if(list[i].max !=0){ %>
			<%=list[i].max %>元</p>
		<% } else { %>
			不限
		<% } %>
		<p class="hd">网点地址: <%=list[i].address %></p>
		<p class="hd">营业时间: <%=list[i].opentime %></p>
		<span class="more"></span>
	</li> 
<%}%>